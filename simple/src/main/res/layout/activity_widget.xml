<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              style="@style/base_match_width_height_with_orientation_vertical"
              android:gravity="center_horizontal"
             >

    <com.chinadragon.commonutilslibrary.widget.CustomClearEditText
        android:id="@+id/et_customclearedittext"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/common_margin_tenSize"
        android:hint="请输入内容"
        android:paddingLeft="@dimen/common_padding_fiveSize"
        android:paddingRight="@dimen/common_padding_fiveSize"
        />

    <TextView
        android:id="@+id/tv_customdialog"
        style="@style/common_textview_style"
        android:text="customdialog"
        />

    <TextView
        android:id="@+id/tv_custompopupwindow"
        style="@style/common_textview_style"
        android:text="custompopupwindow"
        />
</LinearLayout>